magrittr:: set_colnames(value = c( "n", "c", "N", "R")) %>%
as.data.frame()
i = 1
for(n in c(1 : 100)) {
for(c in seq(0.1, 0.9, by = 0.1)) {
results_i <- logistic_model(n = n, c = c, N0 = 90) %>%
filter(time == 50) %>%
group_by(time) %>%
summarise(N = sum(N), R = sum(R))
results$n[i] <- n
results$c[i] <- c
results$N[i] <- results_i$N
results$R[i] <- results_i$R
i = i + 1
}
